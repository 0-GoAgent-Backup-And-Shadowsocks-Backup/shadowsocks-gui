{
    "name": "shadowsocks-gui",
    "main": "index.html",
    "description": "shadowsocks client with GUI",
    "version": "0.5.0",
    "keywords": [ "shadowsocks", "node-webkit" ],
    "single-instance": false,
    "dependencies": {
        "shadowsocks": "1.5.0"
    },
    "window": {
      "icon": "icon.png",
      "toolbar": false,
      "width": 350,
      "height": 420,
      "position": "mouse",
      "min_width": 350,
      "min_height": 420,
      "max_width": 350,
      "max_height": 420
    },
    "js-flags": "--expose-gc",
    "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
    ]
}
